{
    "env": {
        "browser": true,
        "node": true
    },
    "globals": {
        "app": "writable"
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": "src/js/libs",
    "rules": {
        "array-callback-return": 2,
        "no-constant-binary-expression": 2,
        "no-constructor-return": 2,
        "no-duplicate-imports": 2,
        "eqeqeq": 2,
        "semi": 2
    }
}
